doi = {10.1088/0029-5515/55/10/104020},
file = {:C$\backslash$:/Users/joe/Documents/University/York/Fusion{\_}Frontiers/Papers/nf{\_}55{\_}10{\_}104020.pdf:pdf},
issn = {0029-5515},
