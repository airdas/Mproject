@article{Wischmeier2009,
abstract = {A qualitative as well as quantitative simulation of experimentally observed plasma parameters in the detached regime proves to be difficult for several tokamaks. A series of ohmic discharges have been performed in ASDEX Upgrade and DIII-D at as similar as possible plasma parameters and at different line averaged densities, over(n, ̄)e. The experimental data represent a set of well diagnosed discharges against which numerical simulations are compared. For the numerical modelling the fluid-code B2.5 coupled to the Monte Carlo neutrals transport code EIRENE is used. Only the combination of effects, such as geometry, drift terms, neutral conductance, increased radial transport and divertor target composition explains a significant fraction of the experimentally observed ion fluxes, $\Gamma$t, to the inner and outer target plates as a function of over(n, ̄)e in ASDEX Upgrade. The relative importance of the mechanisms leading to detachment differ in DIII-D and ASDEX Upgrade. {\textcopyright} 2009 Elsevier B.V.},
author = {Wischmeier, M. and Groth, M. and Kallenbach, A. and Chankin, A. V. and Coster, D. P. and Dux, R. and Herrmann, A. and M{\"{u}}ller, H. W. and Pugno, R. and Reiter, D. and Scarabosio, A. and Watkins, J. G.},
doi = {10.1016/j.jnucmat.2009.01.081},
file = {:C$\backslash$:/Users/joe/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/2009 - Wischmeier et al. - Current understanding of divertor detachment Experiments and modelling.pdf:pdf},
isbn = {0022-3115},
issn = {00223115},
journal = {Journal of Nuclear Materials},
number = {1},
pages = {250--254},
publisher = {Elsevier B.V.},
title = {{Current understanding of divertor detachment: Experiments and modelling}},
