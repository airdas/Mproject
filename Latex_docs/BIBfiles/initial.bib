journal = {Plasma Physics and Controlled Fusion},
number = {6},
pages = {779--930},
title = {{Experimental divertor physics}},
url = {http://stacks.iop.org/0741-3335/39/i=6/a=001},
volume = {39},
year = {1999}
}
